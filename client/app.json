{
  "expo": {
    "name": "my-baby",
    "slug": "my-baby",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/Logo.png",
     "splash": {
       "image": "./app/assets/MySplash.png",
       "resizeMode": "cover",
       "backgroundColor": "#fff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/Logo.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./app/assets/Logo.png"
    }
  }
}
